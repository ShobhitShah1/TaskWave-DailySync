{
  "expo": {
    "name": "DailySync",
    "slug": "DailySync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.taskwave.dailysync",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "react-native-bootsplash",
      [
        "expo-av",
        {
          "microphonePermission": "Allow to access your microphone."
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": ["Microphone"]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a3b0f09f-94c3-4b22-8934-9cc8e78fc1e4"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/a3b0f09f-94c3-4b22-8934-9cc8e78fc1e4"
    }
  }
}
